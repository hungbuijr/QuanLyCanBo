<p id="notice"><%= notice %></p>

<h1>Que Quans</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Enabled</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @que_quans.each do |que_quan| %>
      <tr>
        <td><%= que_quan.name %></td>
        <td><%= que_quan.enabled %></td>
        <td><%= que_quan.description %></td>
        <td><%= link_to 'Show', que_quan %></td>
        <td><%= link_to 'Edit', edit_que_quan_path(que_quan) %></td>
        <td><%= link_to 'Destroy', que_quan, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Que Quan', new_que_quan_path %>
