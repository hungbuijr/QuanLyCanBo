<p id="notice"><%= notice %></p>

<h1>Boi Duong Qpans</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Enabled</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @boi_duong_qpans.each do |boi_duong_qpan| %>
      <tr>
        <td><%= boi_duong_qpan.name %></td>
        <td><%= boi_duong_qpan.description %></td>
        <td><%= boi_duong_qpan.enabled %></td>
        <td><%= link_to 'Show', boi_duong_qpan %></td>
        <td><%= link_to 'Edit', edit_boi_duong_qpan_path(boi_duong_qpan) %></td>
        <td><%= link_to 'Destroy', boi_duong_qpan, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

