<p id="notice"><%= notice %></p>

<h1>Trinh Do Boi Duongs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Enabled</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @trinh_do_boi_duongs.each do |trinh_do_boi_duong| %>
      <tr>
        <td><%= trinh_do_boi_duong.name %></td>
        <td><%= trinh_do_boi_duong.description %></td>
        <td><%= trinh_do_boi_duong.enabled %></td>
        <td><%= link_to 'Show', trinh_do_boi_duong %></td>
        <td><%= link_to 'Edit', edit_trinh_do_boi_duong_path(trinh_do_boi_duong) %></td>
        <td><%= link_to 'Destroy', trinh_do_boi_duong, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

