<p id="notice"><%= notice %></p>

<h1>Chuc Danhs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Enabled</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @chuc_danhs.each do |chuc_danh| %>
      <tr>
        <td><%= chuc_danh.name %></td>
        <td><%= chuc_danh.description %></td>
        <td><%= chuc_danh.enabled %></td>
        <td><%= link_to 'Show', chuc_danh %></td>
        <td><%= link_to 'Edit', edit_chuc_danh_path(chuc_danh) %></td>
        <td><%= link_to 'Destroy', chuc_danh, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Chuc Danh', new_chuc_danh_path %>
