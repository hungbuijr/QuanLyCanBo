<p id="notice"><%= notice %></p>

<h1>Mon Hocs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Enabled</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mon_hocs.each do |mon_hoc| %>
      <tr>
        <td><%= mon_hoc.name %></td>
        <td><%= mon_hoc.description %></td>
        <td><%= mon_hoc.enabled %></td>
        <td><%= link_to 'Show', mon_hoc %></td>
        <td><%= link_to 'Edit', edit_mon_hoc_path(mon_hoc) %></td>
        <td><%= link_to 'Destroy', mon_hoc, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mon Hoc', new_mon_hoc_path %>
