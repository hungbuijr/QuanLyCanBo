<p id="notice"><%= notice %></p>

<h1>Chuyen Nganhs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Enabled</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @chuyen_nganhs.each do |chuyen_nganh| %>
      <tr>
        <td><%= chuyen_nganh.name %></td>
        <td><%= chuyen_nganh.description %></td>
        <td><%= chuyen_nganh.enabled %></td>
        <td><%= link_to 'Show', chuyen_nganh %></td>
        <td><%= link_to 'Edit', edit_chuyen_nganh_path(chuyen_nganh) %></td>
        <td><%= link_to 'Destroy', chuyen_nganh, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

