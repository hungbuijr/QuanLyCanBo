<p id="notice"><%= notice %></p>

<h1>Ra Khoi Dangs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Enabled</th>
      <th>Ngay ra khoi dang</th>
      <th>Hinh thuc ra khoi dang</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ra_khoi_dangs.each do |ra_khoi_dang| %>
      <tr>
        <td><%= ra_khoi_dang.name %></td>
        <td><%= ra_khoi_dang.description %></td>
        <td><%= ra_khoi_dang.enabled %></td>
        <td><%= ra_khoi_dang.ngay_ra_khoi_dang %></td>
        <td><%= ra_khoi_dang.hinh_thuc_ra_khoi_dang %></td>
        <td><%= link_to 'Show', ra_khoi_dang %></td>
        <td><%= link_to 'Edit', edit_ra_khoi_dang_path(ra_khoi_dang) %></td>
        <td><%= link_to 'Destroy', ra_khoi_dang, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ra Khoi Dang', new_ra_khoi_dang_path %>
