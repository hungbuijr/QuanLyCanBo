<p id="notice"><%= notice %></p>

<h1>Trinh Dos</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Enabled</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @trinh_dos.each do |trinh_do| %>
      <tr>
        <td><%= trinh_do.name %></td>
        <td><%= trinh_do.description %></td>
        <td><%= trinh_do.enabled %></td>
        <td><%= link_to 'Show', trinh_do %></td>
        <td><%= link_to 'Edit', edit_trinh_do_path(trinh_do) %></td>
        <td><%= link_to 'Destroy', trinh_do, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Trinh Do', new_trinh_do_path %>
