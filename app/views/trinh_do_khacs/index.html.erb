<p id="notice"><%= notice %></p>

<h1>Trinh Do Khacs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Enabled</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @trinh_do_khacs.each do |trinh_do_khac| %>
      <tr>
        <td><%= trinh_do_khac.name %></td>
        <td><%= trinh_do_khac.description %></td>
        <td><%= trinh_do_khac.enabled %></td>
        <td><%= link_to 'Show', trinh_do_khac %></td>
        <td><%= link_to 'Edit', edit_trinh_do_khac_path(trinh_do_khac) %></td>
        <td><%= link_to 'Destroy', trinh_do_khac, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

